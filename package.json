{
  "name": "@harshadbhatia/improved-cdk-constructs",
  "version": "0.1.62",
  "description": "Improved aws cdk constructs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/harshadbhatia/improved-cdk-constructs"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf coverage dist pack doc 'lib/**/*.js' 'lib/**/*.d.ts' 'tst/**/*.js' 'tst/**/*.d.ts' 'bin/**/*.d.ts' 'bin/**/*.js' 'interfaces/**/*.js' 'interfaces/**/*.d.ts'",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "release": "standard-version"
  },
  "devDependencies": {
    "@aws-sdk/client-secrets-manager": "^3.99.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.4",
    "@datadog/datadog-api-client": "^1.0.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^10.17.60",
    "@vscode/dev-container-cli": "^0.238.0",
    "aws-cdk": "^2.27.0",
    "axios": "^0.27.2",
    "datadog-cdk-constructs-v2": "^0.3.3",
    "husky": "^7.0.4",
    "ini": "^3.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^9.1.1"
  },
  "dependencies": {
    "@types/ini": "^1.3.31",
    "@types/js-yaml": "^4.0.5",
    "aws-cdk-lib": "^2.28.1",
    "config": "^3.3.7",
    "constructs": "^10.0.74",
    "js-yaml": "^4.1.0",
    "typescript": "^4.7.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
